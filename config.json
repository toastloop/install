{
    "additional-repositories": [
        "multilib"
    ],
    "archinstall-language": "English",
    "audio": "pipewire",
    "bootloader": "systemd-bootctl",
    "config_version": "2.5.0",
    "debug": false,
    "desktop-enviroment": "kde",
    "dry_run": false,
    "gfx_driver": "All open-source (default)",
    "harddrives": [
        "/dev/vda"
    ],
    "hostname": "archtest",
    "kernels": [
        "linux"
    ],
    "keyboard-layout": "us",
    "mirror-region": "United States",
    "mount_point": null,
    "nic": {
        "dhcp": true,
        "dns": null,
        "gateway": null,
        "iface": null,
        "ip": null,
        "type": "nm"
    },
    "ntp": true,
    "packages": [
        "docker", 
        "git", 
        "wget", 
        "zsh"
    ],
    "plugin": null,
    "profile": "gnome",
    "script": "guided",
    "services": ["docker"],
    "silent": false,
    "swap": true,
    "sys-encoding": "UTF-8",
    "sys-language": "en_US",
    "timezone": "America/Denver",
    "version": "2.5.0",
    "custom-commands": [
        "mkdir -p /home/matt/Git; cd /home/matt/Git; git clone https://aur.archlinux.org/paru.git",
        "chown -R matt:matt /home/matt/Git/paru",
        "usermod -aG docker matt",
        "chsh /bin/fish matt"
    ]
}